            background-color: rgb(62, 61, 61, 0.35);
            font-family: 'MinecraftiaRegular';
            font-weight: normal;
            font-style: normal;
            font-size: medium;
        }
        .mpMain {
            border: 1px solid silver;
            width: 100%;
            height: 80%;
            font-family: 'MinecraftiaRegular';
            font-weight: normal;
            font-style: normal;
            font-size: medium;
        }
        .mpFooter {
            border: 1px solid silver;
            text-align: center;
            width: 100%;
            height: 5%;
            font-family: 'MinecraftiaRegular';
            font-weight: normal;
            font-style: normal;
            font-size: medium;
        }
     </style>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
